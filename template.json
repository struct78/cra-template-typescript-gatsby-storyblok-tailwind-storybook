      "npm-check-updates": "^12.3.0",
      "postcss": "^8.4.6",
      "pretty-quick": "^3.1.3",
      "storyblok": "^3.10.4",
      "storyblok-generate-ts": "^1.4.1",
      "ts-jest": "^27.1.3",
      "typescript": "^4.5.5"
    },
    "babelMacros": {
      "twin": {
        "config": "./tailwind.default.js",
        "preset": "emotion"
      }
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    }
  }
