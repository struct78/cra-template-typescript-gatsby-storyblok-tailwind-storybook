      "gatsby-plugin-offline": "^5.7.0",
      "gatsby-plugin-postcss": "^5.7.0",
      "gatsby-plugin-preconnect": "^1.3.0",
      "gatsby-plugin-react-axe": "^0.5.0",
      "gatsby-plugin-react-helmet": "^5.7.0",
      "gatsby-plugin-sharp": "^4.7.0",
      "gatsby-plugin-tailwindcss": "^1.0.3",
      "gatsby-plugin-typescript": "^4.7.0",
      "gatsby-source-filesystem": "^4.7.0",
      "gatsby-source-storyblok": "^3.1.2",
      "gatsby-storyblok-image": "^0.3.0",
      "gatsby-transformer-sharp": "^4.7.0",
      "graphql-request": "^3.6.1",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-helmet": "^6.1.0",
      "react-icons": "^4.3.1",
      "react-lazyload": "^3.2.0",
      "storyblok-react": "^0.1.2",
      "storyblok-rich-text-react-renderer": "^2.5.2",
      "tailwind-vertical-rhythm": "^1.2.0",
      "tailwindcss": "^3.0.22",
      "twin.macro": "^2.8.2"
