{
  "package": {
    "name": "cra-template-typescript-gatsby-storyblok-tailwind-storybook",
    "version": "0.0.2",
    "license": "MIT",
    "scripts": {
      "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true NODE_NO_WARNINGS=1 gatsby develop",
      "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
      "test": "jest",
      "test:watch": "jest --watch",
      "serve": "gatsby serve",
      "storybook": "start-storybook -p 6006",
      "clean": "gatsby clean",
      "build": "gatsby build",
      "build:storybook": "build-storybook",
      "update": "ncu -u",
      "storyblok:login": "storyblok login",
      "storyblok:logout": "storyblok logout",
      "storyblok:pull": "storyblok pull-components --space=145739",
      "storyblok:types": "storyblok-generate-ts source=./components.145739.json target=./src/@types/storyblok"
    },
    "dependencies": {
      "@emotion/react": "^11.7.1",
      "@emotion/styled": "^11.6.0",
      "@loadable/component": "^5.15.2",
