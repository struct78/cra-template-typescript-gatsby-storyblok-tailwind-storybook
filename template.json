      "@storyofams/storyblok-toolkit": "^1.6.9",
      "emotion-reset": "^3.0.1",
      "gatsby": "^4.7.1",
      "gatsby-plugin-emotion": "^7.7.0",
      "gatsby-plugin-google-fonts": "^1.0.1",
      "gatsby-plugin-image": "^2.7.0",
      "gatsby-plugin-loadable-components-ssr": "^4.2.0-beta.0",
